{
    "name": "refine-directus-v5",
    "description": "Refine Directus REST data provider. This is a wrapper around the Directus SDK to use in refine.",
    "version": "1.0.39",
    "license": "MIT",
    "typings": "dist/index.d.ts",
    "type": "module",
    "main": "dist/refine-directus-v5.esm.js",
    "repository": "git://github.com/TomCaserta/refine-directus-v5",
    "private": false,
    "files": [
        "dist",
        "src"
    ],
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "start": "tsdx watch --tsconfig tsconfig.json --verbose --noClean",
        "build": "tsdx build --tsconfig tsconfig.json",
        "test": "tsdx test --passWithNoTests --runInBand",
        "size": "size-limit",
        "analyze": "size-limit --why"
    },
    "peerDependencies": {
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
        "@refinedev/core": "^5.0.5"
    },
    "author": "Vivek Palanisamy",
    "size-limit": [
        {
            "path": "dist/refine-directus.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/refine-directus.esm.js",
            "limit": "10 KB"
        }
    ],
    "devDependencies": {
        "@size-limit/preset-small-lib": "^5.0.5",
        "size-limit": "^5.0.5",
        "tsdx": "^0.14.1",
        "tslib": "^2.3.1"
    },
    "dependencies": {
        "@directus/sdk": "^16.0.0",
        "@types/react": "^18.2.20",
        "pluralize": "^8.0.0"
    }
}
